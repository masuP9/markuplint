{
	"tag": "area",
	"attributes": [
		{
			"name": "alt",
			"type": "string",
			"required": true,
			"deps": ["href"]
		},
		{
			"name": "coords",
			"type": "coords"
		},
		{
			"name": "download",
			"type": "string"
		},
		{
			"name": "href",
			"type": "URL"
		},
		{
			"name": "ping",
			"type": "URLList"
		},
		{
			"name": "referrerpolicy",
			"type": "string",
			"enum": [
				"no-referrer",
				"no-referrer-when-downgrade",
				"origin",
				"origin-when-cross-origin",
				"strict-origin-when-cross-origin",
				"unsafe-url"
			]
		},
		{
			"name": "rel",
			"type": "LinkTypeList",
			"deps": ["href"]
		},
		{
			"name": "shape",
			"type": "string",
			"enum": ["rect", "circle", "poly", "default"]
		},
		{
			"name": "target",
			"type": "string",
			"deps": ["href"]
		},
		{
			"name": "type",
			"type": "string"
		}
	]
}
